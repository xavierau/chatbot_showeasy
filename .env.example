APP_ENV=development
DSPY_LLM_DEFAULT_PROVIDER=gemini
DSPY_LLM_DEFAULT_MODEL=gemini-2.5-flash

GOOGLE_API_KEY=your_google_api_key_here

#langfuse
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key
LANGFUSE_SECRET_KEY=your_langfuse_secret_key
LANGFUSE_HOST=http://localhost:3002

# database
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=your_database_password
DB_NAME=showeasy

# api server
API_SERVER_HOST=0.0.0.0
API_SERVER_PORT=3010

# Event platform configuration
# Base URL for event platform (used in event URLs, help links, membership pages)
# For development/testing: https://eventplatform.test
# For production: https://showeasy.ai
EVENT_PLATFORM_BASE_URL=https://showeasy.ai

# Guardrail configuration
GUARDRAIL_STRICT_MODE=true
GUARDRAIL_AUTO_SANITIZE=true
GUARDRAIL_LOG_VIOLATIONS=true
